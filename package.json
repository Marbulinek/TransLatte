{
  "name": "@marbulinek/translatte",
  "version": "1.1.0",
  "description": "Automated translation generator for JavaScript/TypeScript projects (Angular, React, Vue) using Lingva Translate API",
  "main": "dist/index.js",
  "bin": {
    "translatte": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.{ts,js}' 'tests/**/*.{ts,js}' --max-warnings=0",
    "format": "prettier --check .",
    "preversion": "npm test && npm run build",
    "version": "git add -A",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "translation",
    "i18n",
    "internationalization",
    "angular",
    "react",
    "vue",
    "ngx-translate",
    "react-i18next",
    "vue-i18n",
    "lingva",
    "translate",
    "localization",
    "l10n",
    "json",
    "typescript"
  ],
  "author": "Lukas Caniga",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "prettier": "^3.2.5",
    "semantic-release": "^24.2.9",
    "ts-jest": "^29.4.5",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Marbulinek/TransLatte.git"
  },
  "bugs": {
    "url": "https://github.com/Marbulinek/TransLatte/issues"
  },
  "homepage": "https://github.com/Marbulinek/TransLatte#readme"
}
